<h1>AngularTest#index</h1>
<p>Find me in app/views/angular_test/index.html.erb</p>
<article ng-app="angular_test">
  <header ng-if="name">
    <h1>Hello, {{name}}</h1>
  </header>
  <section>
    <form class="form-inline">
      <div class="form-group">
        <label for="name">Name: </label>
        <input type="text" class="form-control" placeholder="Who are you looking for?"
        name="name" autofocus ng-model="name">
      </div>
    </form>
  </section>
</article>
